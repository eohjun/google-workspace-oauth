# Google OAuth credentials
GOOGLE_CLIENT_ID=your_client_id_here
GOOGLE_CLIENT_SECRET=your_client_secret_here

# Railway will set this automatically, or set manually for local development
REDIRECT_URI=https://your-railway-domain.railway.app/callback

# Refresh token (for automatic token refresh on server restart)
GOOGLE_REFRESH_TOKEN=your_refresh_token_here

# Port (Railway will set this automatically)
PORT=3000
